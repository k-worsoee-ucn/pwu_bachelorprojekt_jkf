# Make a .env file in the backend directory, by copying this example and filling in the values

# Database Connection URL (for Prisma)
DATABASE_URL=postgresql://postgres:your_secure_password_here@database:5432/sales_process

# Application Port
PORT=3000

# Security
JWT_SECRET=your_jwt_secret_here

# Encryption
ENCRYPTION_KEY=your_encryption_key_here

# CORS
CORS_ORIGIN=http://localhost:5173

# Registration Security
REGISTRATION_ACCESS_CODE=your_access_code_here

# Seed Data
SEED_PASSWORD=your_seed_password_here
